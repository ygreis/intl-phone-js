{
  "name": "intl-phone-js",
  "version": "0.1.9",
  "description": "Headless international phone input engine with smart masking, validation and country auto-detection powered by libphonenumber-js.",
  "license": "MIT",
  "author": "ygreis",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ygreis/intl-phone-js.git"
  },
  "homepage": "https://github.com/ygreis/intl-phone-js#readme",
  "bugs": {
    "url": "https://github.com/ygreis/intl-phone-js/issues"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "dev": "webpack serve --config webpack.playground.js",
    "build:playground": "webpack --config webpack.playground.js",
    "build": "tsup src/index.ts --format esm,cjs --dts --minify",
    "build:watch": "tsup src/index.ts --format esm,cjs --dts --watch",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "phone-input",
    "intl-phone",
    "phone-mask",
    "phone-validation",
    "phone-number",
    "libphonenumber",
    "international-phone",
    "country-detection",
    "headless",
    "typescript"
  ],
  "dependencies": {
    "libphonenumber-js": "^1.12.37"
  },
  "devDependencies": {
    "@types/jsdom": "^28.0.0",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "@vitest/coverage-v8": "^4.0.18",
    "css-loader": "^7.1.2",
    "eslint": "^10.0.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "html-webpack-plugin": "^5.6.3",
    "jsdom": "^28.1.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.8.1",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.2",
    "tsup": "^8.5.1",
    "typescript": "^5.8.2",
    "vitest": "^4.0.18",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.1"
  }
}
