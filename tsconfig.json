{
    "compilerOptions": {
      "target": "es5", // Para compatibilidade com navegadores
      "module": "ES6", // Garante a exportação no formato ES6
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
  }